package atividade3;

public class atividade3 {

	public static void main(String[] args) {
		String test = "aqui teste aqui teste aqui aqui";

		int cont = 0;
		
		while(true) {

			int index = test.indexOf("aqui");
			
			if(index != -1) {
				cont++;
				test = test.replaceFirst("aqui", "*");
			}else {
				
				break;
			}

			
		}
		
		System.out.println(cont);
	}
}
